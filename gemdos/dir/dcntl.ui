!begin_node Bindings for Dcntl
!ignore_index
(!begin_liste) [Assembler:]
!item [C:]
int32_t Dcntl ( int16_t cmd, int8_t *name, int32_t arg );

!item [Assembler:]
!begin_verbatim
move.l    arg,-(sp)    ; Offset 8
pea       name         ; Offset 4
move.w    cmd,-(sp)    ; Offset 2
move.w    #304,-(sp)   ; Offset 0
trap      #1           ; GEMDOS
lea       $C(sp),sp    ; Correct stack
!end_verbatim
(!ende_liste)
!end_node
