int16_t Dsetpath ( CONST int8_t *path );
!item [Assembler:]
!begin_verbatim
pea       path         ; Offset 2
move.w    #59,-(sp)    ; Offset 0
trap      #1           ; GEMDOS
addq.l    #6,sp        ; correct stack
!end_verbatim

!item [GFA-Basic]
Fehler%=Gemdos(&H3B,L:path%)

(!ende_liste)
!end_node
