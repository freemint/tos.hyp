!item [C:]
void *wdlg_get_udata ( DIALOG *dialog );

!item [Binding:]
!begin_verbatim
void *wdlg_get_udata ( DIALOG *dialog )
{
   addr_in[0] = dialog;
   int_in[0] = 2;
   crys_if (164);

   return ( addr_out[0] );
}
!end_verbatim

