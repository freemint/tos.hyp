void lbox_set_bentries ( LIST_BOX *box, int16_t entries );

!item [Binding:]
!begin_verbatim
void lbox_set_bentries ( LIST_BOX *box, int16_t entries )
{
   addr_in[0] = box;

   int_in[0] = 6;
   int_in[1] = entries;
   crys_if (175);
}
!end_verbatim

