!item [C:]
int16_t objc_add ( OBJECT *ob_atree, int16_t ob_aparent, int16_t ob_achild );

!item [Binding:]
!begin_verbatim
int16_t objc_add (OBJECT *ob_atree, int16_t ob_aparent,
                  int16_t ob_achild)
{
   int_in[0]  = ob_aparent;
   int_in[1]  = ob_achild;
   addr_in[0] = ob_atree;

   return ( crys_if(40) );
}
!end_verbatim

