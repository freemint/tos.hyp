!item [Binding:]
!begin_verbatim
void v_setrgbi( int16_t handle, int16_t primtype,
                int16_t r, int16_t g, int16_t b, int16_t i )
{
   intin[0] = primtype;
   intin[1] = r;
   intin[2] = g;
   intin[3] = b;
   intin[4] = i;

   contrl[0] = 5;
   contrl[1] = 0;
   contrl[3] = 5;
   contrl[5] = 18500;
   contrl[6] = handle;

   vdi ();
}
