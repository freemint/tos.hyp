!ignore_index

(!begin_liste) [GEM-Arrays]

!item [C:]
int16_t vsf_interior ( int16_t handle, int16_t style );

!item [Binding:]
!begin_verbatim
int16_t vsf_interior (int16_t handle, int16_t style)
{
   intin[0] = style;

   contrl[0] = 23;
   contrl[1] = 0;
   contrl[3] = 1;
   contrl[6] = handle;

   vdi ();

   return ( intout[0] );
}
!end_verbatim

