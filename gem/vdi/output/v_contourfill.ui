!item [Binding:]
!begin_verbatim
void v_contourfill (int16_t handle, int16_t x, int16_t y,
                    int16_t index)
{
   intin[0]  = index;
   ptsin[0]  = x;
   ptsin[1]  = y;
   contrl[0] = 103;
   contrl[1] = 1;
   contrl[3] = 1;
   contrl[6] = handle;

   vdi ();
}
!end_verbatim

