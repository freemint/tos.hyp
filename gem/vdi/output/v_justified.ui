intin+4   !! intin[2..n+1] !! string[0..n-1]
ptsin     !! ptsin[0]      !! x
ptsin+2   !! ptsin[1]      !! y
ptsin+4   !! ptsin[2]      !! length
ptsin+6   !! ptsin[3]      !! 0
intout    !! intout[0..n-1] !! char_with[0..n-1]
~         !! ~              !! ~~~~~~~ if word_space 0x8000 or 0x8001
!end_table

(!ende_liste)
!end_node

