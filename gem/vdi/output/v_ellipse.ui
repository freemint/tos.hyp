!item [Binding:]
!begin_verbatim
void v_ellipse (int16_t handle, int16_t x, int16_t y,
                int16_t xradius, int16_t yradius)
{
   ptsin[0]  = x;
   ptsin[1]  = y;
   ptsin[2]  = xradius;
   ptsin[3]  = yradius;

   contrl[0] = 11;
   contrl[1] = 2;
   contrl[3] = 0;
   contrl[5] = 5;
   contrl[6] = handle;

   vdi ();
}
!end_verbatim

