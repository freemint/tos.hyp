void vr_recfl (int16_t handle, int16_t *pxyarray)
{
   ptsin[0..3] = pxyarray[0..3];

   contrl[0] = 114;
   contrl[1] = 2;
   contrl[3] = 0;
   contrl[6] = handle;

   vdi ();
}
!end_verbatim

