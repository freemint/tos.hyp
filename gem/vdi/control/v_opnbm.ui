contrl+6  !! contrl[3]     !! 20    # entry in intin
contrl+8  !! contrl[4]     !! 45    # entry in intout
contrl+10 !! contrl[5]     !! 1     Sub-Opcode
contrl+12 !! contrl[6]     !! handle
contrl+14 !! contrl[7..8]  !! bitmap
intin     !! intin[0..19]  !! work_in[0..19]
