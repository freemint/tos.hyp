!item [C:]
void vst_unload_fonts ( int16_t handle, int16_t select );

!item [Binding:]
!begin_verbatim
void vst_unload_fonts (int16_t handle, int16_t select)
{
   intin[0] = select;

   contrl[0] = 120;
   contrl[1] = 0;
   contrl[3] = 1;
   contrl[6] = handle;

   vdi ();
}
!end_verbatim

