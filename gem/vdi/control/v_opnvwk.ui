!item [C:]
void v_opnvwk ( int16_t *work_in, int16_t *handle, int16_t *work_out );

!item [Binding:]
!begin_verbatim
void v_opnvwk (int16_t *work_in, int16_t *handle,
               int16_t *work_out)
{
   intin[0..10] = work_in[0..10]

   contrl[0] = 100;
   contrl[1] = 0;
   contrl[3] = 11;
   contrl[6] = *handle;

   vdi ();

   *handle = contrl[6]; 
   work_out[0..44]  = intout[0..44];
   work_out[45..56] = ptsout[0..11];
}
!end_verbatim

