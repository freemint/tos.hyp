!iflang [english]

!begin_node vqf_attributes

(!begin_liste) [Availability]

!item [Name:]
¯Inquire Current Fill Area Attributes® - ermittelt die Attribute zum
Ausfllen von Fl„chen.

!item [Opcode:]
37

!item [Syntax:]
void vqf_attributes ( int16_t handle, int16_t *attrib );

!item [Description:]
This function reports back the current attributes for filled objects.

!begin_xlist !short [Parameter]
!item [Parameter]
Meaning
!item [~]
~
!item [handle]
Device handle
!item [attrib[0!]]
fill type
!item [attrib[1!]]
fill color
!item [attrib[2!]]
fill style index
!item [attrib[3!]]
writing mode
!item [attrib[4!]]
perimeter flag
!begin_table [rl]
0 = !! invisible
<> 0 = !! visible
!end_table
!end_xlist

!item [(!nolink [Return]) Value:]
Die Funktion liefert kein Ergebnis.

!item [Availability:]
Supported by all devices.

!item [Group:]
Inqurie function

!item [See Also:]
(!link [Binding] [Bindings for vqf_attributes])  ~   vswr_mode ~   vs_color  ~  vsf_interior ~
vsf_style ~  vsf_color  ~  vsf_perimeter
               
(!ende_liste)
!end_node



!begin_node Bindings for vqf_attributes
!ignore_index

(!begin_liste) [GEM-Arrays]

!item [C:]
void vqf_attributes ( int16_t handle, int16_t *attrib );

!item [Binding:]
!begin_verbatim
void vqf_attributes (int16_t handle, int16_t *attrib)
{
   contrl[0] = 37;
   contrl[1] = 0;
   contrl[3] = 0;
   contrl[6] = handle;

   vdi ();

   attrib[0..4] = intout[0..4];
}
!end_verbatim

!item [GEM-Arrays:]
!begin_table [l l l]
Address !! Element !! Contents
!hline
