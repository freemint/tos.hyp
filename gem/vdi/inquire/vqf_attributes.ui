contrl+2  !! contrl[1]    !! 0     # entry in ptsin
contrl+4  !! contrl[2]    !! 0     # entry in ptsout
contrl+6  !! contrl[3]    !! 0     # entry in intin
contrl+8  !! contrl[4]    !! 5     # entry in intout
contrl+12 !! contrl[6]    !! handle
intout    !! intout[0..4] !! attrib[0..4]
!end_table

(!ende_liste)
!end_node

