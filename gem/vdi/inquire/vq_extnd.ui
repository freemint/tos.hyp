!item [Binding:]
!begin_verbatim
void vq_extnd (int16_t handle, int16_t owflag, int16_t *work_out)
{
   intin[0] = owflag;

   contrl[0] = 102;
   contrl[1] = 0;
   contrl[3] = 1;
   contrl[6] = handle;

   vdi ();

   work_out[0..44]  = intout[0..44];
   work_out[45..56] = ptsout[0..11];
}
!end_verbatim

