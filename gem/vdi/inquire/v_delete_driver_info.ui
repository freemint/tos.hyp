{
  *(DRV_INFO **) &intin[0] = drv_info;

  contrl[0] = 181;
  contrl[1] = 0;
  contrl[3] = 2;
  contrl[5] = 0;
  contrl[6] = handle;

  vdi ();

  return( intout[0] );
}
