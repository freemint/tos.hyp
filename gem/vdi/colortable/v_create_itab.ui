contrl+6  !! contrl[3]    !! 5     # Entry in intin
contrl+8  !! contrl[4]    !! 2     # Entry in intout
contrl+10 !! contrl[5]    !! 0     # Sub-opcode
contrl+12 !! contrl[6]    !! handle
