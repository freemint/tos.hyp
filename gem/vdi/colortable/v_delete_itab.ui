!begin_node v_delete_itab

(!begin_liste) [Availability]

!item [Name:]
¯Delete inverse colour table reference®

!item [Opcode:]
208 (Sub-Opcode 1)

!item [Syntax:]
int16_t  v_delete_itab( int16_t handle, ITAB_REF *itab );

!item [Description:]
The call v_delete_itab releases the memory used for an inverse colour table.

!begin_xlist !short [Parameter]
!item [Parameter]
Meaning
!item [~]
~
!item [handle]
Workstation identifier

!item [itab]
Reference to inverse colour table

!end_xlist


!item [(!nolink [Return]) value:]
0: Error (!nl)
1: All OK

!item [Availability:]
As of NVDI 5.00.

!item [Group:]
Colour table functions

!item [See also:]
(!link [Binding] [Bindings for v_delete_itab])

(!ende_liste)
!end_node



!begin_node Bindings for v_delete_itab
!ignore_index

(!begin_liste) [GEM-Arrays]

!item [C:]
int16_t  v_delete_itab( int16_t handle, ITAB_REF *itab );

!item [Binding:]
!begin_verbatim
int16_t  v_delete_itab( int16_t handle, ITAB_REF *itab )
{
   intin[0..1] = itab; 

   contrl[0] = 208;
   contrl[1] = 0;
   contrl[3] = 2;
   contrl[5] = 1;
   contrl[6] = handle;

   vdi ();

   return ( intout[0] );
}
!end_verbatim

