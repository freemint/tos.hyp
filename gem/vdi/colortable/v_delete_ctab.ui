!iflang [english]

!begin_node v_delete_ctab

(!begin_liste) [Availability]

!item [Name:]
¯Delete colour table®

!item [Opcode:]
206 (Sub-Opcode 9)

!item [Syntax:]
int16_t v_delete_ctab( int16_t handle, COLOR_TAB *ctab );

!item [Description:]
The call v_delete_ctab clears a colour table created with v_create_ctab.

!begin_xlist !short [Parameter]
!item [Parameter]
Meaning
!item [~]
~
!item [handle]
Workstation identifier

!item [ctab]
Pointer to colour table

!end_xlist

!item [(!nolink [Return]) value:]
0: Error (!nl)
1: All OK

                                     
!item [Availability:]
As of NVDI 5.00.

!item [Group:]
Colour table functions

!item [See also:]
(!link [Binding] [Bindings for v_delete_ctab]) ~ v_create_ctab

(!ende_liste)
!end_node



!begin_node Bindings for v_delete_ctab
!ignore_index

(!begin_liste) [GEM-Arrays]

!item [C:]
int16_t v_delete_ctab( int16_t handle, COLOR_TAB *ctab );

!item [Binding:]
!begin_verbatim
int16_t v_delete_ctab( int16_t handle, COLOR_TAB *ctab )
{
   intin[0..1] = ctab; 

   contrl[0] = 206
   contrl[1] = 0;
   contrl[3] = 2;
   contrl[5] = 9;
   contrl[6] = handle;

   vdi ();

   return ( intout[0] );
}
!end_verbatim

