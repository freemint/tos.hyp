contrl+6  !! contrl[3] !! 1     # entry in intin
contrl+8  !! contrl[4] !! 0     # entry in intout
contrl+10 !! contrl[5] !! 62
contrl+12 !! contrl[6] !! handle
intin     !! intin[0]  !! action
intout    !! intout[0] !! Return Value
