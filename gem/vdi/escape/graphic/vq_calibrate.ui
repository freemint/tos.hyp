!item [Binding:]
!begin_verbatim
int16_t vq_calibrate (int16_t handle, int16_t *flag)
{
   contrl[0] = 5;
   contrl[1] = 0;
   contrl[3] = 1;
   contrl[5] = 77;
   contrl[6] = handle;

   vdi ();

   *flags = intout[0];
   return (  contrl[4] );
}
!end_verbatim

!item [GEM-Arrays:]
