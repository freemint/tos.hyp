contrl+6  !! contrl[3]   !! 0     # entry in intin
contrl+8  !! contrl[4]   !! 0     # entry in intout
contrl+10 !! contrl[5]   !! 21
contrl+12 !! contrl[6]   !! handle
ptsin     !! ptsin[0..3] !! xyarray[0..3]
!end_table

(!ende_liste)
!end_node

