!item [C:]
void vr_trnfm ( int16_t handle, MFDB *psrcMFDB, MFDB *pdesMFDB );

!item [Binding:]
!begin_verbatim
void vr_trnfm (int16_t handle, MFDB *psrcMFDB, MFDB *pdesMFDB)
{
   iptr (psrcMFDB);
   iptr_2 (pdesMFDB);

   contrl[0] = 110;
   contrl[1] = 0;
   contrl[3] = 0;
   contrl[6] = handle;

   vdi ();
}
!end_verbatim

