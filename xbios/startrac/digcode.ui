!iflang [english]


!begin_node digcode
(!begin_liste) [Availability]

!item [Name:]
ØdigcodeÆ - Digital input record-type code

!item [Opcode:]
500, 41

!item [Syntax:]
int32_t xbios ( 500, 41 );

!item [Description:]

!item [(!nolink [Return]) value:]
!begin_xlist [bit 0 ]!short
!item [bit 0]  0 = Professional (!nl)
               1 = Consumer
!item [bit 1]  0 = Data (!nl)
               1 = Audio
!item [bit 2]  0 = No copy-prohibit = SCMS not active (!nl)
               1 = Copy prohibit=SCMS active
!item [bit 3]  0 = No (!nolink [emphasis]) (!nl)
               1 = (!nolink [Emphasis]) active
!end_xlist

!item [Availability:]
This function is only available when the driver of the (!nolink [StarTrack])
sound card has been launched.

!item [Group:]
Sound routines

!item [See also:]
(!link [Binding] [Bindings for digcode])

(!ende_liste)


!begin_node Bindings for digcode
!ignore_index
(!begin_liste) [Assembler:]
!item [C:]
int32_t xbios ( 500, 41 );
!item [Assembler:]
!begin_verbatim
move.w    #41,-(sp)    ; Offset 2
move.w    #500,-(sp)   ; Offset 0
trap      #14          ; Call XBIOS
addq.l    #4,sp        ; Correct stack
!end_verbatim
(!ende_liste)
!end_node
!end_node


