!item [Gruppe:]
Soundroutinen

!item [Querverweis:]
(!link [Binding] [Bindings fr wr_dport])

(!ende_liste)


!begin_node Bindings fr wr_dport
!ignore_index
(!begin_liste) [Assembler:]
!item [C:]
int32_t xbios ( 500, 232, int16_t wert );
!item [Assembler:]
!begin_verbatim
move.w    wert,-(sp)   ; Offset 4
move.w    #232,-(sp)   ; Offset 2
move.w    #500,-(sp)   ; Offset 0
