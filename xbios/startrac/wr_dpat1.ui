!iflang [english]


!begin_node wr_dpath1
(!begin_liste) [Availability]

!item [Name:]
¯wr_dpath1® - Write dsp dpath1 register

!item [Opcode:]
500, 230

!item [Syntax:]
int32_t xbios ( 500, 230, int16_t value );

!item [Description:]
!begin_ilist [value  ] !short
!item [value] bit 0 -> SW0PATH (!nl)
           bit 1 -> SC1PATH (!nl)
           bit 2 -> SC2PATH (!nl)
           bit 3 -> DSPRES
!end_ilist

!item [(!nolink [Return]) value:]
0 - No error

!item [Availability:]
This function is only available when the driver of the (!nolink [StarTrack])
sound card has been launched.

!item [Group:]
Sound routines

!item [See also:]
(!link [Binding] [Bindings for wr_dpath1])

(!ende_liste)


!begin_node Bindings for wr_dpath1
!ignore_index
(!begin_liste) [Assembler:]
!item [C:]
int32_t xbios ( 500, 230, int16_t value );
!item [Assembler:]
!begin_verbatim
move.w    value,-(sp)  ; Offset 4
move.w    #230,-(sp)   ; Offset 2
move.w    #500,-(sp)   ; Offset 0
trap      #14          ; Call XBIOS
addq.l    #6,sp        ; Correct stack
!end_verbatim
(!ende_liste)
!end_node
!end_node


