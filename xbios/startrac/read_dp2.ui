!iflang [english]


!begin_node read_dpath2
(!begin_liste) [Availability]

!item [Name:]
¯read_dpath2® - Read dsp dpath2 register

!item [Opcode:]
500, 221

!item [Syntax:]
int32_t xbios ( 500, 221 );

!item [Description:]

!item [(!nolink [Return]) value:]
bit 0 -> SRDPATH (!nl)
bit 1 -> STDPATH0 (!nl)
bit 2 -> STDPATH1 (!nl)
bit 3 -> CONVMAST

!item [Availability:]
This function is only available when the driver of the (!nolink [StarTrack])
sound card has been launched.

!item [Group:]
Sound routines

!item [See also:]
(!link [Binding] [Bindings for read_dpath2])

(!ende_liste)


!begin_node Bindings for read_dpath2
!ignore_index
(!begin_liste) [Assembler:]
!item [C:]
int32_t xbios ( 500, 221 );
!item [Assembler:]
!begin_verbatim
move.w    #221,-(sp)   ; Offset 2
move.w    #500,-(sp)   ; Offset 0
trap      #14          ; Call XBIOS
addq.l    #4,sp        ; Correct stack
!end_verbatim
(!ende_liste)
!end_node
!end_node


