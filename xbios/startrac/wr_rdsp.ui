!iflang [english]


!begin_node wr_rdsp
(!begin_liste) [Availability]

!item [Name:]
¯wr_rdsp® - Write dsp register

!item [Opcode:]
500, 212

!item [Syntax:]
int32_t xbios ( 500, 212, int16_t value );

!item [Description:]
!begin_ilist [value  ] !short
!item [value] bit 0 -> MATI0/REC4 (!nl)
           bit 1 -> MATI1/PLAY4 (!nl)
           bit 2 -> VOLCS1 (!nl)
           bit 3 -> VOLCLK (!nl)
           bit 4 -> VOLDATAI (!nl)
           bit 5 -> VOLCS2 (!nl)
           bit 7 -> AESFREQ
!end_ilist

!item [(!nolink [Return]) value:]
0 - No error

!item [Availability:]
This function is only available when the driver of the (!nolink [StarTrack])
sound card has been launched.

!item [Group:]
Sound routines

!item [See also:]
(!link [Binding] [Bindings for wr_rdsp])

(!ende_liste)


!begin_node Bindings for wr_rdsp
!ignore_index
(!begin_liste) [Assembler:]
!item [C:]
int32_t xbios ( 500, 212, int16_t value );
!item [Assembler:]
!begin_verbatim
move.w    value,-(sp)  ; Offset 4
move.w    #212,-(sp)   ; Offset 2
move.w    #500,-(sp)   ; Offset 0
trap      #14          ; Call XBIOS
addq.l    #6,sp        ; Correct stack
!end_verbatim
(!ende_liste)
!end_node
!end_node


