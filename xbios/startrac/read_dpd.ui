!ignore_index
(!begin_liste) [Assembler:]
!item [C:]
int32_t xbios ( 500, 224 );
!item [Assembler:]
!begin_verbatim
move.w    #224,-(sp)   ; Offset 2
move.w    #500,-(sp)   ; Offset 0
trap      #14          ; Call XBIOS
addq.l    #4,sp        ; Correct stack
!end_verbatim
(!ende_liste)
!end_node
!end_node


