!iflang [english]


!begin_node wr_raes
(!begin_liste) [Availability]

!item [Name:]
¯wr_raes® - Write aes register

!item [Opcode:]
500, 213

!item [Syntax:]
int32_t xbios ( 500, 213, int16_t value );

!item [Description:]
!begin_ilist [value  ] !short
!item [value] bit 0 -> SWPROF (!nl)
           bit 1 -> SWCOPY/PCH16 (!nl)
           bit 2 -> DIGMODE:SWMAL2/DIG24 (!nl)
           bit 3 -> PLAY128 (!nl)
           bit 4 -> REC128 (!nl)
           bit 7 -> SPEBU
!end_ilist

!item [(!nolink [Return]) value:]
0 - No error

!item [Availability:]
This function is only available when the driver of the (!nolink [StarTrack])
sound card has been launched.

!item [Group:]
Sound routines

!item [See also:]
(!link [Binding] [Bindings for wr_raes])

(!ende_liste)


!begin_node Bindings for wr_raes
!ignore_index
(!begin_liste) [Assembler:]
!item [C:]
int32_t xbios ( 500, 213, int16_t value );
!item [Assembler:]
!begin_verbatim
move.w    value,-(sp)  ; Offset 4
move.w    #213,-(sp)   ; Offset 2
move.w    #500,-(sp)   ; Offset 0
trap      #14          ; Call XBIOS
addq.l    #6,sp        ; Correct stack
!end_verbatim
(!ende_liste)
!end_node
!end_node


