(!ende_liste)


!begin_node Bindings for peaksuch
!ignore_index
(!begin_liste) [Assembler:]
!item [C:]
int32_t xbios ( 500, 301, int32_t anfmem, int32_t endmem );
!item [Assembler:]
!begin_verbatim
move.l    endmem,-(sp)  ; Offset 8
move.l    anfmem,-(sp)  ; Offset 4
move.w    #301,-(sp)    ; Offset 2
