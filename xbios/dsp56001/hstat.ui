!ignore_index
(!begin_liste) [Assembler:]
!item [C:]
#include <tos.h>

int8_t Dsp_HStat( void );
!item [Assembler:]
!begin_verbatim
move.w    #125,-(sp)   ; Offset 0
trap      #14          ; XBIOS aufrufen
addq.l    #2,sp        ; Stack korrigieren
