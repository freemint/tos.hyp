 #include <tos.h>

int16_t Dsp_GetProgAbility( void );
!item [Assembler:]
!begin_verbatim
move.w    #114,-(sp)   ; Offset 0
trap      #14          ; Call XBIOS
addq.l    #2,sp        ; Correct stack
!end_verbatim
(!ende_liste)
!end_node
!end_node


