!item [C:]
int32_t vdxPlaybackOff ( void );
!item [Assembler:]
!begin_verbatim
move.w    #408,-(sp)   ; Offset 0
trap      #14          ; Call XBIOS
addq.l    #2,sp        ; Correct stack
!end_verbatim
(!ende_liste)
!end_node
