!ignore_index
(!begin_liste) [Assembler:]
!item [C:]
 #include <tos.h>

VPOS Vseek( int16_t x, int16_t y );
!item [Assembler:]
!begin_verbatim
move.w    #78,-(sp)   ; Offset 0
trap      #14         ; Call XBIOS
addq.l    #2,sp       ; Correct stack
!end_verbatim
(!ende_liste)
!end_node
