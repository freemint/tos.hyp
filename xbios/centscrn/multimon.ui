!iflang [english]


!begin_node MultiMon
(!begin_liste) [Availability]

!item [Name:]
ØMultiMonÆ - Multisynchro mode

!item [Opcode:]
82

!item [Syntax:]
int16_t MultiMon( int16_t Cmd );

!item [Description:]
You can authorise the switch between RGB and VGA.

!begin_table [r|l]
    Cmd !! Description 
!hline
     0 !! Forbid the switch.
    >0 !! Authorize the switch. 
    -1 !! The current mode. 
!end_table

!item [(!nolink [Return]) value:]
The old mode.

!item [Availability:]
CENTScreen

!item [Group:]
CENTScreen XBIOS extension

!item [See also:]
(!link [Binding] [Bindings for MultiMon])

(!ende_liste)


!begin_node Bindings for MultiMon
!ignore_index
(!begin_liste) [Assembler:]
!item [C:]
#include <tos.h>

int16_t MultiMon( int16_t Cmd );
!item [Assembler:]
!begin_verbatim
move.w    #Cmd,-(sp)  ; Offset 2
move.w    #82,-(sp)   ; Offset 0
trap      #14         ; Call XBIOS
addq.l    #4,sp       ; Correct stack
!end_verbatim
(!ende_liste)
!end_node
!end_node


