!begin_node Bindings fr WavePlay
!ignore_index
(!begin_liste) [Assembler:]
!item [C:]
LONG WavePlay ( WORD flags, LONG rate, LONG sptr, LONG slen );
!item [Assembler:]
!begin_verbatim
move.l    slen,-(sp)
pea       sptr
move.l    rate,-(sp)
move.w    flags,-(sp)
move.w    #165,-(sp)   ; Offset 0
trap      #14          ; XBIOS aufrufen
addq.l    #16,sp       ; Stack korrigieren
!end_verbatim
(!ende_liste)
!end_node
